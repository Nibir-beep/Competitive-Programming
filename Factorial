#include <stdio.h>


long long factorial(int N) {
    long long result = 1;
    for (int i = 1; i <= N; i++) {
        result *= i;
    }
    return result;
}

int main() {
    int T;  
    scanf("%d", &T);  // Read the number of test cases
    
    while (T--) {
        int N;
        scanf("%d", &N);  
        printf("%lld\n", factorial(N));  
    }
    
    return 0;
}
