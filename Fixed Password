#include <stdio.h>

int main() {
    int X;
    
    while (scanf("%d", &X)) {  // Read until we encounter the correct password
        if (X == 1999) {
            printf("Correct\n");
            break;  // Exit the loop once the correct password is entered
        } else {
            printf("Wrong\n");
        }
    }
    
    return 0;
}
